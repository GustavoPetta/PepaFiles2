Projeto 1

#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()

{
    double a, b;
    printf("Quais os numeros que deseja somar ? ");
    scanf("%lf", &a);
    scanf("%lf", &b);
    a = a+b;
    if (a > 10){
            printf("o resultado e: %lf ", a);

    }

    return 0;
 }
  


Projeto 2

#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()

{
    double a, b, c, d;
    printf("Qual o seu salario ? ");
    scanf("%lf", &a);
    printf("Qual o valor do emprestimo que deseja ? ");
    scanf("%lf", &b);
    printf("Em quantos meses deseja pagar ? ");
    scanf("%lf", &c);
    d = (b/c)/a *100;
     if (d > 30){
            printf("o seu emprestimo foi negado");
            return 0;

    }

    printf("o seu emprestimo aprovado");
    return 0;
 }


Projeto 3

#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()

{
    int x;
    printf("Qual o numero ? ");
    scanf("%d", &x);
    if (x%2 == 0){
    printf("o resultado e um numero par" );
    return 0;
    }

    printf("o resultado e um numero impar" );

    return 0;
 }



projeto 4

#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()

{
    double a, b, c;
    printf("Qual o numero ? ");
    scanf("%lf", &a);
    b = sqrt(a);
    c = pow(a,2);
    
    if (a>0)
    {
    printf("o resultado e %lf", b );
    return 0;
    }

    printf("o resultado e %lf", c );
    return 0;
 }


Projeto 5

#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()

{
    double a, b;
    printf("Quais os numeros ? ");
    scanf("%lf", &a);
    scanf("%lf", &b);

    if (a > b){
    printf("o maior e: %lf ", a);
    return 0;
    }

    if (b > a){
    printf("o maior e: %lf ", b);
    return 0;
    }

    printf("os numeros sao iguais a: %lf ", a);
    return 0;
    }


Projeto 6

#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()

{
    double a, b, c;
    printf("Quais os numeros ? ");
    scanf("%lf", &a);
    scanf("%lf", &b);
    scanf("%lf", &c);

    if (a > b && a > c){
        if(b > c){
            printf("%lf %lf %lf ", c, b, a);
            return 0;
        } else {
            printf("%lf %lf %lf ", b, c, a);
        }
    }

    if (b> a && b > c){
         if(a > c)
    {printf("%lf %lf %lf ", c, a, b);
    return 0;
    } else { printf("%lf %lf %lf ", a,c,b);}
    }

    if (c> a && c > b){
    if(a > b)
    {printf("%lf %lf %lf ", b, a, c);
    return 0;
    } else { printf("%lf %lf %lf ", a,b,c);}
    }

    return 0;
    }


Projeto 7

#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()

{
    double a;
    printf("Qual o numero? ");
    scanf("%lf", &a);
    if (a >= 20 && a <= 90){
            printf("Sim, o numero: %lf, esta entre 20 e 90 ", a);
      return 0;
    }
     printf("Nao, o numero: %lf, nao esta entre 20 e 90 ", a);
    return 0;
 }
